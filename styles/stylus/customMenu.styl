.container
    clear left
    margin 10px
    padding 10px
    div
        float left

.start_pointy_corners
    height 100px
    width 75px
    background #d15e00
    position relative
    margin-right -25px !important
    &:before
        width 0
        content ''
        position absolute
        top 0
        right 0
        border-top 50px solid #ffffff
        border-left 25px solid #d15e00
        border-bottom 50px solid #ffffff

.end_pointy_corners
    height 100px
    width 50px
    background #d15e00
    position relative
    margin-left 26px
    &:before
        width 0
        content ''
        position absolute
        top 0
        left -25px
        border-top 50px solid #d15e00
        border-right 25px solid transparent
        border-bottom 50px solid #d15e00
        transform rotate(180deg)

.middle_pointy_corners
